extend vue

snippet eldate "el-date-picker"
 <el-date-picker
  placeholder="选择日期"
  v-model="pickedDate"
  type="date"
  :readonly="false"
  :disabled="false"
  @change="onDatePickerChange"
  />
endsnippet

snippet eldatep "el-date-picker with picker-options"
<el-date-picker
  placeholder="选择日期"
  v-model="pickedDate"
  type="date"
  align="right"
  :picker-options="datePickerOptions"
  :readonly="false"
  :disabled="false"
  @change="onDatePickerChange"
  />
endsnippet

snippet eldatepi "el-date-picker with type of week"
<el-date-picker
  placeholder="选择周"
