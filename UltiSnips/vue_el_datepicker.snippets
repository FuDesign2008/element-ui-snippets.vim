snippet eldatepickerso "el-date-picker with start/end date and picker-options"
 <el-date-picker
  start-placeholder="开始日期"
  range-separator="至"
  end-placeholder="结束日期"
  v-model="startAndEndDate"
  type="daterange"
  align="right"
  unlink-panels
  :picker-options="pickerOptions"
  :readonly="false"
