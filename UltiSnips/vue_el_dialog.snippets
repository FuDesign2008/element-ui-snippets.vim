
extends vue

snippet eldialog "el-dialog" b
<el-dialog
  title="提示"
  :visible.sync="dialogVisible"
  :fullscreen="false"
  width="30%"
  :before-close="handleClose"
>
  <span>这是一段信息</span>
  <span
    slot="footer"
    class="dialog-footer"
  >
    <el-button @click="handleCancel">取 消</el-button>
    <el-button
      type="primary"
      @click="handleConfirm"
    >
      确 定
    </el-button>
  </span>
</el-dialog>
endsnippet
