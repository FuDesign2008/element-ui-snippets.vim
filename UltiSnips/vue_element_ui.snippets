snippet eltimeselect "el-time-select"
<el-time-select
  v-model="timeValue"
  :picker-options="{
    start: '08:30',
    step: '00:15',
    end: '18:30'
  }"
  placeholder="选择时间">
</el-time-select>
endsnippet

snippet eltimeselectrange "el-time-select with range"
 <el-time-picker
    v-model="timeValue"
    :picker-options="{
      selectableRange: '18:30:00 - 20:30:00'
    }"
    placeholder="任意时间点">
  </el-time-picker>
endsnippet

snippet eltimeselectboth "el-time-select with start time and end time"
  <el-time-select
    placeholder="起始时间"
    v-model="startTime"
    :picker-options="{
      start: '08:30',
      step: '00:15',
      end: '18:30'
    }">
  </el-time-select>
  <el-time-select
    placeholder="结束时间"
    v-model="endTime"
    :picker-options="{
      start: '08:30',
      step: '00:15',
      end: '18:30',
      minTime: startTime
    }"
	>
  </el-time-select>
endsnippet

snippet eltimepicker "el-time-picker"
<el-time-picker
	is-range
	v-model="startEndTimeArray"
	range-separator="至"
	start-placeholder="开始时间"
	end-placeholder="结束时间"
	placeholder="选择时间范围">
</el-time-picker>
endsnippet

