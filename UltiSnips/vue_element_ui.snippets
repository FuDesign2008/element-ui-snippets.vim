snippet elautocomplete "el-autocomplete"
<el-autocomplete
  v-model="inputValue"
  :fetch-suggestions="fetchSuggestions"
  placeholder="请输入内容"
  @select="handleSelect"
></el-autocomplete>
endsnippet

snippet elautocompletesolt "el-autocomplete with slot"
<el-autocomplete
  popper-class="my-autocomplete"
  v-model="inputValue"
  :fetch-suggestions="fetchSuggestions"
  placeholder="请输入内容"
  @select="handleSelect">
  <i
    class="el-icon-edit el-input__icon"
    slot="suffix"
    @click="handleIconClick">
  </i>
  <template slot-scope="{ item }">
    <div class="name">{{ item.value }}</div>
    <span class="addr">{{ item.address }}</span>
  </template>
</el-autocomplete>
endsnippet

snippet elinputnumber "el-input-number"
<el-input-number
	v-model="inputNumber"
	@change="handleChange"
	:min="1"
	:max="10"
	:step="1"
	label="描述文字">
</el-input-number>
endsnippet

snippet elselect "el-select"
<el-select v-model="value" placeholder="请选择" clearable filterable>
    <el-option
      v-for="item in options"
      :key="item.value"
      :label="item.label"
      :value="item.value"
	  :disabled="item.disabled || false"
	  >
    </el-option>
</el-select>
endsnippet

snippet elselectmultiple "el-select with multiple"
<el-select
    v-model="selectValues"
    multiple
    collapse-tags
    placeholder="请选择">
    <el-option
      v-for="item in options"
      :key="item.value"
      :label="item.label"
      :value="item.value">
    </el-option>
</el-select>
endsnippet

snippet elselectcustom "el-select with custom el-option"
  <el-select v-model="value6" placeholder="请选择">
    <el-option
      v-for="item in cities"
      :key="item.value"
      :label="item.label"
      :value="item.value">
      <span style="float: left">{{ item.label }}</span>
      <span style="float: right; color: #8492a6; font-size: 13px">{{ item.value }}</span>
    </el-option>
  </el-select>
endsnippet

snippet elselectgroup "el-select-group"
<el-select v-model="selectValue" placeholder="请选择">
    <el-option-group
      v-for="group in groupList"
      :key="group.label"
      :label="group.label">
      <el-option
        v-for="item in group.options"
        :key="item.value"
        :label="item.label"
        :value="item.value">
      </el-option>
    </el-option-group>
</el-select>
endsnippet

snippet elselectremote "el-select with remote data"
<el-select
    v-model="selectValue"
    multiple
    filterable
    remote
    reserve-keyword
    placeholder="请输入关键词"
    :remote-method="fetchRemoteOptions"
    :loading="loading">
    <el-option
      v-for="item in options"
      :key="item.value"
      :label="item.label"
      :value="item.value">
    </el-option>
</el-select>
endsnippet

snippet elcascader "el-cascader"
<el-cascader
    placeholder="请选择"
    expand-trigger="hover"
    :options="options"
    v-model="selectValue"
	:show-all-levels="false"
	:change-on-select="false"
    @change="handleChange">
</el-cascader>
endsnippet

snippet elcascaderfilterable "el-cascader with filterable"
  <el-cascader
    placeholder="试试搜索：指南"
    :options="options"
    v-model="selectValue"
    filterable
	:show-all-levels="false"
    :change-on-select="false"
  ></el-cascader>
endsnippet

