snippet eloptionslot "el-option with default slot"
<el-option
  v-for="item in options"
  key={ item.value }
  value={ item.value }
  disabled={ item.disabled }
  >
  <span
    style="float: left"
    >{{item.lablel}}</span>
  <span
    style="float: right; color: #8492a6; font-size: 13px"
    >{{ item.lablel2 }}</span>
</el-option>
endsnippet

snippet eloptiongroup "el-option-group"
 <el-option-group
  v-for="group in optionGroupList"
  key={ group.label }
  label={ group.label }
  >
  <el-option
    v-for="item in group.options"
    key={ item.value }
    label={ item.label }
    value={ item.value }
    disabled={ false }
    />
</el-option-group>
endsnippet

