extends vue

snippet elselect "el-select"
<el-select
  v-model={ selectedValue }
  placeholder="请选择"
  disabled={ false }
  clearable={ false }
  onChange={ onSelectChange }
  >
    <el-option
      v-for="item in options"
      key={ item.value }
      label={ item.label }
      value={ item.value }
      disabled={ item.disabled }
      />
</el-select>
endsnippet

snippet elselectmulti "el-select with multiple attribute"
<el-select
  v-model={ selectedValue }
  placeholder="请选择"
  multiple
  multiple-limit={ 0 }
  disabled={ false }
  collapse-tags={ false }
  onChange={ onSelectChange }
  >
    <el-option
      v-for="item in options"
      key={ item.value }
      label={ item.label }
      value={ item.value }
      disabled={ item.disabled }
      />
</el-select>
endsnippet

